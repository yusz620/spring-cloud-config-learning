<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.cloud.config.demo.dao.mapper.UserMapper">
	<select id="selectOne" parameterType="spring.cloud.config.demo.domain.User" resultType="spring.cloud.config.demo.domain.User">
	   select * from users where id=#{id};
	</select>
	
	<insert id="insertOne" parameterType="spring.cloud.config.demo.domain.User" >
	   insert into users;
	</insert>
	
</mapper>